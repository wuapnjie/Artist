            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".waterfall.WaterfallActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".elasticdrag.ElasticActivity"
            android:theme="@style/AppTheme.Translucent" />
        <activity android:name=".twowayseek.TwoWayActivity" />
        <activity android:name=".emojiexplode.EmojiExplodeActivity" />
        <activity android:name=".interactionimage.InteractionImageActivity" />
        <activity android:name=".liveprogress.LiveProgressActivity" />
        <activity android:name=".manyanimator.ManyAnimatorActivity" />
    </application>

</manifest>